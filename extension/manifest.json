{
  "manifest_version": 3,
  "name": "Moodle Palette",
  "version": "0.1",
  "description": "Keyboard shortcuts and command palette for Moodle",
  "content_scripts": [
    {
      "matches": ["https://moodle.calvin.edu/mod/assign/view.php*"],
      "js": ["content/keyboard-rubric-grading.js"],
      "run_at": "document_idle",
      "world": "MAIN"
    },
    {
      "matches": ["https://moodle.calvin.edu/mod/forum/view.php*"],
      "js": ["content/discussion-full-credit.js"],
      "run_at": "document_idle",
      "world": "MAIN"
    }
  ]
}
