{
  "manifest_version": 2,
  "name": "Moodle Palette",
  "version": "1.0.0",
  "description": "Adds keyboard navigation to Moodle.",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://moodle.calvin.edu/*"],
      "js": ["content_scripts/content_script.js"]
    }
  ]
}